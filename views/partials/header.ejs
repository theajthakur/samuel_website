<style>
    <% if (locals.primaryColor && locals.secondaryColor && locals.backgroundColor) { %>
    :root {
      --primary-color: <%= primaryColor %>;
      --secondary-color: <%= secondaryColor %>;
      --background-color: <%= backgroundColor %>;
    }
    <% } else { %>
      :root {
      --primary-color: rgb(255, 182, 59);
      --secondary-color: rgb(2, 26, 54);
      --background-color: rgb(0,0,0);
    }
  <% } %>
    body {
      overflow-x: hidden;
      font-family: "Quicksand", sans-serif;
    }
    h1,
    h2,
    h3,
    h4,
    g5,
    h6,
    p {
      font-family: "Quicksand", sans-serif;
    }

    .abs-ctr {
      display: flex;
      align-items: center;
      gap: 10px;
    }

    .tbl-ctr {
      width: 100%;
      overflow-x: scroll;
      text-align: center;
    }

    .form-group {
      display: block;
      margin: 10px 0;
    }

    .text-right {
      text-align: right;
    }

    ::-webkit-scrollbar {
      display: none;
    }

    .nobreakoverflow {
      white-space: nowrap;
      width: 100%;
      overflow-x: scroll;
    }

    .card .card-header {
      background: var(--primary-color);
      color: black;
      text-align: center;
    }

    .card {
      border-color: var(--primary-color);
      margin: 5px 0;
    }

    .card .card-footer {
      background: white;
    }

    .hover-zoom {
      transition: 0.2s;
      padding: 0 calc(var(--bs-gutter-x));
      cursor: pointer;
    }

    .hover-zoom:hover {
      padding: 0 calc(var(--bs-gutter-x) * 0.5);
    }

    .bg-vj_primary {
      background: var(--primary-color) !important;
    }
    .bg-vj_secondary {
      background: var(--secondary-color) !important;
    }
    .bg-vj_background {
      background: var(--background-color) !important;
    }
    .btn-vj_primary {
      --bs-btn-color: var(--background-color);
      --bs-btn-bg: var(--primary-color); /* Background color of the button */
      --bs-btn-hover-color: var(--background-color); /* Text color on hover */
      --bs-btn-hover-bg: var(--primary-color);
    }
    .btn-vj_secondary {
      --bs-btn-color: var(--background-color);
      --bs-btn-bg: var(--secondary-color); /* Background color of the button */
      --bs-btn-hover-bg: var(--secondary-color); /* Background color on hover */
      --bs-btn-hover-color: var(--background-color);
    }

    .alert-custom {
      --bs-alert-color: var(--background-color);
      --bs-alert-bg: var(--primary-color);
      --bs-alert-border-color: var(--primary-color);
      --bs-alert-link-color: var(--secondary-color);
      opacity: 0.8 !important;
    }
</style>
